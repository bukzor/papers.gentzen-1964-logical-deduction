#!/usr/bin/env python3
from typing import Iterator

Lines = Iterator[str]

BEGIN = '<thinking>\n'
END = '</thinking>\n'

def claude_ocr_strip(lines: Lines):
    for line in lines:
        if line == BEGIN:
            for line in lines:
                if line == END:
                    break
        else:
            print(line, end='')


def main():
    import sys
    return claude_ocr_strip(sys.stdin)


if __name__ == '__main__':
    raise SystemExit(main())
